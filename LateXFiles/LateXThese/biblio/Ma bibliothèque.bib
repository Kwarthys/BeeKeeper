@article{damala_loupe_2016,
	title = {The Loupe: Tangible Augmented Reality For Learning To Look At Ancient Greek Art},
	volume = {16},
	rights = {Creative Commons Attribution 4.0, Open Access},
	url = {https://zenodo.org/record/204970},
	doi = {10.5281/ZENODO.204970},
	shorttitle = {The Loupe},
	abstract = {With the advent of digital museum interactives as a widely available learning offer in all types of museums, including history of art and archaeology museums, an ongoing debate has been established: Do these usually screen-based - museum interactives assist visitors in focusing on museum objects and artefacts? Or do they distract and take away the attention from the real museum objects on display? We present the Loupe, a tangible Augmented Reality prototype in form of a magnifying lens, which allows museum visitors to get information in context about museum artefacts. We detail the design and content creation process that was employed in order to create a thematic tour for the Greek Gallery of Allard Pierson Museum in Amsterdam. An evaluation study with 22 adult participants was carried out, using both qualitative and quantitative evaluation methods, so as to explore the utility and usability of the Loupe as well its learning and affective impact. Our findings suggest that the acceptance of the Loupe as a museum interactive and learning resource, was related both with its qualities as a tangible as well as with the structure of the content and the narratives revealed.},
	pages = {73--85},
	number = {5},
	journaltitle = {Mediterranean Archaeology and Archaeometry},
	author = {Damala, Areti and Hornecker, Eva and van der Vaart, Merel and van Dijk, Dick and Ruthven, Ian},
	urldate = {2021-09-14},
	date = {2016},
	langid = {english},
	note = {Publisher: Zenodo},
	keywords = {Augmented Reality, Emotions, Evaluation, Learning, Museum Visit, Tangible Interaction, User Experience},
	file = {Damala et al. - 2016 - The Loupe Tangible Augmented Reality For Learning.pdf:C\:\\Users\\Kwarthys\\Zotero\\storage\\HLK7ETAV\\Damala et al. - 2016 - The Loupe Tangible Augmented Reality For Learning.pdf:application/pdf}
