	langid = {english},
	file = {Perelman et al. - 2015 - The Roly-Poly Mouse Designing a Rolling Input Dev.pdf:C\:\\Users\\Kwarthys\\Zotero\\storage\\T7HB2ZGX\\Perelman et al. - 2015 - The Roly-Poly Mouse Designing a Rolling Input Dev.pdf:application/pdf}
